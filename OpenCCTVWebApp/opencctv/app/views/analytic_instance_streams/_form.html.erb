<%= form_for([@analytic_instance, @analytic_instance_stream]) do |f| %>
      <% if @analytic_instance_stream.errors.any? %>
        <div id="error_explanation" class="alert alert-danger" role="alert">
      <h2><%= pluralize(@analytic_instance_stream.errors.count, "error") %> prohibited this analytic instance input stream from being saved:</h2>

      <ul>
      <% @analytic_instance_stream.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :analytic_input_stream_id, "Analytic Input Stream"%><br>
    <%= f.collection_select( :analytic_input_stream_id, @analytic_instance.analytic.analytic_input_streams, :id, :name, {prompt: 'Select Input Stream Type'})%>
  </div>

    <div class="field">
      <%= f.label :stream_id, "Stream"%><br>
      <%= f.collection_select( :stream_id, Stream.all, :id, :name, {prompt: 'Select the Stream'})%>
    </div>
    <br/>
    <div class="actions">
      <%= f.submit class: "btn btn-success btn-mini"%>
    </div>
<% end %>
