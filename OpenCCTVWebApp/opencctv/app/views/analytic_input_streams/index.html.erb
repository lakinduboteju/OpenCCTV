<h1>Listing Analytic Input Streams</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @analytic_input_streams.each do |analytic_input_stream| %>
      <tr>
        <td><%= analytic_input_stream.name %></td>
        <td><%= analytic_input_stream.description %></td>
        <td><%= link_to 'Show', analytic_input_stream %></td>
        <td><%= link_to 'Edit', edit_analytic_input_stream_path(analytic_input_stream) %></td>
        <td><%= link_to 'Destroy', analytic_input_stream, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Analytic Input Stream', new_analytic_input_stream_path %>
